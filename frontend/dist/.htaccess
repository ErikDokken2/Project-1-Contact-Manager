<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /myvuejsapp/Project-1-Contact-Manager/frontend/dist/
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /myvuejsapp/Project-1-Contact-Manager/frontend/dist/index.html [L]
</IfModule>